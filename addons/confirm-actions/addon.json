{
  "name": "确认提示",
  "description": "在Scratch账号进行提交操作时，询问您是否确认操作，例如分享项目。",
  "credits": [
    {
      "name": "TheColaber",
      "link": "https://scratch.mit.edu/users/TheColaber"
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/*"],
      "runAtComplete": false
    }
  ],
  "settings": [
    {
      "name": "分享项目时提示",
      "id": "projectsharing",
      "type": "boolean",
      "default": true
    },
    {
      "name": "关注/取消关注时提示",
      "id": "followinguser",
      "type": "boolean",
      "default": false
    },
    {
      "name": "加入工作室时提示",
      "id": "joiningstudio",
      "type": "boolean",
      "default": false
    }
  ],
  "tags": ["community", "recommended", "studios"],
  "enabledByDefault": false,
  "l10n": true
}
