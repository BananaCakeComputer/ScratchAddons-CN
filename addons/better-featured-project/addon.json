{
  "name": "个人信息页面背景图片",
  "description": "使所有人的个人信息页面上的精选作品替换成一个好看的图像。",
  "credits": [
    {
      "name": "Ezlambry",
      "link": "https://ezlambry.github.io/"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/users/*/"]
    }
  ],
  "userstyles": [
    {
      "url": "userstyle.css",
      "matches": ["https://scratch.mit.edu/users/*/"]
    }
  ],
  "settings": [
    {
      "id": "blur",
      "name": "背景图像模糊度",
      "type": "integer",
      "min": 0,
      "max": 5,
      "default": 0
    }
  ],
  "tags": ["community", "recommended", "profiles"],
  "l10n": true
}
